{
  "name": "calladina",
  "version": "1.0.0",
  "description": "AI Voice Receptionist Setup Application - Full Stack",
  "main": "index.js",
  "scripts": {
    "dev": "concurrently \"npm run backend\" \"npm run frontend\"",
    "backend": "cd backend && npm run dev",
    "frontend": "cd frontend && npm start",
    "install:backend": "cd backend && npm install",
    "install:frontend": "cd frontend && npm install",
    "install:all": "npm run install:backend && npm run install:frontend",
    "build": "cd frontend && npm run build",
    "start:backend": "cd backend && npm start",
    "start:frontend": "cd frontend && npm start",
    "setup": "npm run install:all && echo 'Setup complete! Create .env files in both frontend and backend directories using the .env.example templates.'"
  },
  "keywords": [
    "ai",
    "voice",
    "receptionist",
    "twilio",
    "firebase",
    "react",
    "express"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "concurrently": "^9.2.0"
  },
  "engines": {
    "node": ">=14.0.0"
  }
}
